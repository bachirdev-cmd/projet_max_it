{
    "name": "bachir-uchiwa/projetmaxit",
    "description": "Application bancaire MaxIT avec v√©rification CNI",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BachirUchiwa\\Projetmaxit\\": "src/",
            "App\\Entity\\": "src/entity/",
            "App\\Repository\\": "src/repository/",
            "App\\Service\\": "src/service/",
            "App\\Controller\\": "src/controller/",
            "App\\Core\\": "app/core/",
            "App\\Config\\": "app/config/",
            "App\\Config\\Middleware\\": "app/config/middleware/",
            "App\\Core\\Abstract\\": "app/core/abstract/",
            "Route\\": "routes/"
        }
    },
    "authors": [
        {
            "name": "Uchiwa75567",
            "email": "pabassdiame76@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "vlucas/phpdotenv": "^5.6",
        "symfony/yaml": "*",
        "ext-curl": "*",
        "ext-pdo": "*",
        "ext-pdo_pgsql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "database:migrate": "php console/migrate.php",
        "database:seed": "php console/seed.php",
        "setup:production": "php setup-production.php",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
